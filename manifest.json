{
    "name": "Sugarchain Wallet Extension",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["js/wallet.js"]
        }
    ],
    "browser_action": {
        "default_popup": "overlay.html",
        "default_title": "Sugarchain Wallet",
        "default_icon": "img/logo.png"
    }
}